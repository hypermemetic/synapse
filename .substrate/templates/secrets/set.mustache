{{! hyperforge.org.juggernautlabs.secrets.set - returns: secrets.SecretEvent }}
{{! Variants: listed, retrieved, updated, prompt_required, acquire_started, acquired, error }}
{{#listed}}
  keys: {{#keys}}
    is_set={{is_set}} key={{key}} provider={{provider}}{{/keys}}
  org_name: {{org_name}}
{{/listed}}
{{#retrieved}}
  key: {{key}}
  org_name: {{org_name}}
  value: {{value}}
{{/retrieved}}
{{#updated}}
  key: {{key}}
  org_name: {{org_name}}
{{/updated}}
{{#prompt_required}}
  key: {{key}}
  message: {{{message}}}
  org_name: {{org_name}}
{{/prompt_required}}
{{#acquire_started}}
  forge: {{forge}}
  org_name: {{org_name}}
{{/acquire_started}}
{{#acquired}}
  key: {{key}}
  org_name: {{org_name}}
  source: {{source}}
{{/acquired}}
{{#error}}
  key: {{#key}}{{.}}{{/key}}{{^key}}null{{/key}}
  message: {{{message}}}
  org_name: {{org_name}}
{{/error}}
